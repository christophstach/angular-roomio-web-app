{
  "name": "roomio",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run database\" ",
    "tsc": "tsc",
    "database": "json-server --port 3501 data/roomio.json",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.0.0",
    "bootstrap-social": "^4.11.0",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.3",
    "angular2": "2.0.0-beta.17",
    "systemjs": "0.19.26",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "json-server": "^0.8.10",
    "typescript": "^1.8.10",
    "typings":"^0.8.1"
  }
}
